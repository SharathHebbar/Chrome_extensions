{
    "manifest_version": 3,
    "name": "RAG Chrome Extension",
    "version": "1.0",
    "description": "Ask questions to your RAG app.",
    "permissions": ["activeTab", "storage"],
    "host_permissions": ["http://localhost:5000/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  